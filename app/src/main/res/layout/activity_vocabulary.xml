                android:id="@+id/vocabulary_learn_vocabularys"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="8dp"
                android:text="Original - Übersetzung"
                android:textAlignment="center"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/vocabulary_learn_languages" />

        </android.support.constraint.ConstraintLayout>
    </android.support.constraint.ConstraintLayout>

    <android.support.constraint.ConstraintLayout
        android:id="@+id/vocabulary_ShareMainView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ScrollView
            android:id="@+id/vocabulary_ShareScollView0"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:visibility="visible"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <android.support.constraint.ConstraintLayout
                android:id="@+id/vocabulary_BaseShareView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:visibility="visible"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <android.support.constraint.ConstraintLayout
                    android:id="@+id/vocabulary_FirstShareView"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent">

                    <Button
                        android:id="@+id/vocabulary_ShareSearch"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="8dp"
                        android:layout_marginTop="8dp"
                        android:layout_marginEnd="16dp"
                        android:layout_marginBottom="8dp"
                        android:text="Vokabellisten Suchen"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="@+id/vocabulary_ShareGuideline0"
                        app:layout_constraintTop_toBottomOf="@+id/vocabulary_ShareEditView"
                        app:layout_constraintVertical_bias="0.0" />

                    <TextView
                        android:id="@+id/vocabulary_ShareHeadline"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="8dp"
                        android:layout_marginTop="8dp"
                        android:layout_marginEnd="8dp"
                        android:text="Öffentliche Vokabellisten"
                        android:textAlignment="center"
                        android:textSize="24sp"
                        android:textStyle="bold"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <android.support.design.widget.TextInputLayout
                        android:id="@+id/vocabulary_ShareEditView"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="8dp"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="1.0"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/vocabulary_ShareHeadline">

                        <android.support.design.widget.TextInputEditText
                            android:id="@+id/vocabulary_ShareGetList"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:hint="Zum Suchen ID, Name oder Sprachen eingeben"
                            android:textAlignment="center"
                            android:textSize="18sp"
                            android:textStyle="bold" />
                    </android.support.design.widget.TextInputLayout>

                    <Button
                        android:id="@+id/vocabulary_ShareBack"
                        android:layout_width="0dp"
                        android:layout_height="0dp"
                        android:layout_marginStart="16dp"
                        android:layout_marginTop="8dp"
                        android:layout_marginEnd="8dp"
                        android:layout_marginBottom="8dp"
                        android:text="Zurück"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toStartOf="@+id/vocabulary_ShareGuideline0"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/vocabulary_ShareEditView" />

                    <android.support.constraint.Guideline
                        android:id="@+id/vocabulary_ShareGuideline0"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        app:layout_constraintGuide_percent="0.50" />

                </android.support.constraint.ConstraintLayout>

                <android.support.constraint.ConstraintLayout
                    android:id="@+id/vocabulary_SecondShareView"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/vocabulary_FirstShareView"
                    app:layout_constraintVertical_bias="0.0">

                    <TextView
                        android:id="@+id/vocabulary_ShareRecommended"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="8dp"
                        android:layout_marginTop="8dp"
                        android:layout_marginEnd="8dp"
                        android:layout_marginBottom="24dp"
                        android:text="Empfohlene Vokabellisten:"
                        android:textAlignment="center"
                        android:textSize="24sp"
                        android:textStyle="bold"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                </android.support.constraint.ConstraintLayout>

            </android.support.constraint.ConstraintLayout>
        </ScrollView>

        <ScrollView
            android:id="@+id/vocabulary_ShareScrollView1"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/vocabulary_ShareScollView0">


            <android.support.constraint.ConstraintLayout
                android:id="@+id/vocabulary_ShareScrollMainLayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal"
                android:weightSum="2">

                <android.support.constraint.Guideline
                    android:id="@+id/vocabulary_ShareGuideline3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    app:layout_constraintGuide_percent="0.5" />

                <android.support.constraint.Guideline
                    android:id="@+id/vocabulary_ShareGuideline4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    app:layout_constraintGuide_percent="0.96" />

                <android.support.constraint.Guideline
                    android:id="@+id/vocabulary_ShareGuideline2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    app:layout_constraintGuide_percent="0.04" />

                <Button
                    android:id="@+id/vocabulary_ShareScrollBase0"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="#D8D8D8"
                    android:text="0"
                    app:layout_constraintEnd_toStartOf="@+id/vocabulary_ShareGuideline3"
                    app:layout_constraintStart_toStartOf="@+id/vocabulary_ShareGuideline2"
                    app:layout_constraintTop_toTopOf="parent" />

                <Button
                    android:id="@+id/vocabulary_ShareScrollBase1"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:background="#D8D8D8"
                    android:text="1"
                    app:layout_constraintEnd_toStartOf="@+id/vocabulary_ShareGuideline4"
                    app:layout_constraintStart_toEndOf="@+id/vocabulary_ShareScrollBase0"
                    app:layout_constraintTop_toTopOf="parent" />

                <Button
                    android:id="@+id/vocabulary_ShareScrollBase2"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:background="#D8D8D8"
                    android:text="2"
                    app:layout_constraintEnd_toStartOf="@+id/vocabulary_ShareGuideline3"
                    app:layout_constraintStart_toStartOf="@+id/vocabulary_ShareGuideline2"
                    app:layout_constraintTop_toBottomOf="@+id/vocabulary_ShareScrollBase0" />

                <Button
                    android:id="@+id/vocabulary_ShareScrollBase3"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:background="#D8D8D8"
                    android:text="3"
                    app:layout_constraintEnd_toStartOf="@+id/vocabulary_ShareGuideline4"
                    app:layout_constraintHorizontal_bias="0.0"
                    app:layout_constraintStart_toStartOf="@+id/vocabulary_ShareGuideline3"
                    app:layout_constraintTop_toBottomOf="@+id/vocabulary_ShareScrollBase1" />

            </android.support.constraint.ConstraintLayout>
        </ScrollView>

    </android.support.constraint.ConstraintLayout>

</android.support.constraint.ConstraintLayout>