                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="@+id/guideline18"
                app:layout_constraintTop_toBottomOf="@+id/timetable_dayName" />
        </android.support.constraint.ConstraintLayout>

        <android.support.constraint.ConstraintLayout
